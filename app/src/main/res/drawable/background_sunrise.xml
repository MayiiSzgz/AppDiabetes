<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Fondo del cielo -->
    <gradient
        android:startColor="#FFDD00"
        android:endColor="#FF9000"
        android:angle="90" />

    <!-- Sol -->
    <shape android:shape="oval">
        <solid android:color="#FFF97F00" />
        <size
            android:width="150dp"
            android:height="150dp" />
    </shape>

    <!-- Montañas -->
    <shape>
        <solid android:color="#FF5E401D" />
        <corners android:radius="0dp" />
    </shape>

    <!-- Río -->
    <shape>
        <solid android:color="#FF1D71B8" />
    </shape>

    <!-- Árboles -->
    <shape>
        <solid android:color="#FF006400" />
    </shape>

    <!-- Gato -->
    <shape>
        <solid android:color="#FF000000" />
    </shape>
</shape>
